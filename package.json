{
  "name": "react-native-navigation-deck",
  "title": "React Native Deck",
  "version": "2.0.2",
  "nativePackage": true,
  "description": "Double deck layout",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "build": "rm -rf ./lib && tsc",
    "prestart": "npm run build",
    "prepare": "npm run build",
    "tsc": "tsc",
    "lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "start": "react-native start",
    "run:ios": "react-native run-ios --project-path ./example/ios",
    "start:android": "adb shell am start -n com.navigation.deck.example/.MainActivity",
    "run:android": "cd example/android && ./gradlew installDebug && npm run start:android",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/listenzz/react-native-navigation-deck.git"
  },
  "author": "listen <listenzz@163.com>",
  "homepage": "https://github.com/listenzz/react-native-navigation-deck",
  "readme": "https://github.com/listenzz/react-native-navigation-deck#readme",
  "keywords": [
    "react-native",
    "navigation",
    "deck"
  ],
  "license": "MIT",
  "licenseFilename": "LICENSE",
  "readmeFilename": "README.md",
  "peerDependencies": {
    "react": "^16.8.1",
    "react-native": ">=0.60.4",
    "react-native-navigation-hybrid": ">=0.19.5"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/runtime": "^7.4.3",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.0",
    "@types/react": "^16.8.14",
    "@types/react-native": "^0.62.13",
    "@types/react-test-renderer": "16.9.0",
    "babel-jest": "^26.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.5.0",
    "jest": "^26.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react": "16.11.0",
    "react-native": "^0.62.2",
    "react-native-navigation-hybrid": "^0.27.0",
    "react-test-renderer": "16.11.0",
    "typescript": "^3.7.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
